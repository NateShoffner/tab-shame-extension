{
    "name": "Tabs count",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Counts the number of open browser tabs",
    "action": {
        "default_title": "Tab count",
        "default_icon": "icons/icons8-add-tab-96.png"
    },
    "permissions": ["notifications", "nativeMessaging"],
    "icons": {"96": "icons/icons8-add-tab-96.png"},
    "content_security_policy": {"extension_pages": "default-src 'self';"},
    "background": {"service_worker": "js/background.js"}
}
